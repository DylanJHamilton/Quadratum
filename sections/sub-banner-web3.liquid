{% comment %}
  Sub Page Banner — Web3 Variant
  File: sections/sub-banner-web3.liquid
  Section class: q-sub-banner-web3
  Section ID (name): "Sub Page Banner (Web3)"
{% endcomment %}

{% liquid
  assign s = section.settings

  assign M_autoplay  = settings.video_autoplay | default: true
  assign M_loop      = settings.video_loop | default: true
  assign M_muted     = settings.video_muted | default: true
  assign M_controls  = settings.video_controls | default: false

  assign respect_rm  = settings.reduce_motion_mode | default: true

  assign do_autoplay = M_autoplay
  if respect_rm
    assign do_autoplay = false
  endif

  assign is_slideshow = false
  if s.mode == 'slideshow' and section.blocks.size > 0
    assign is_slideshow = true
  endif

  assign align_h = s.align_h | default: 'center'
  assign aria_label = s.aria_label | default: s.heading | default: 'Sub page banner'
%}

<section
  class="q-sub-banner-web3 qsbw3 align-{{ align_h }}"
  role="region"
  aria-label="{{ aria_label | escape }}"
  data-q-hero="sub-web3"
  data-q-hero-variant="sub-banner"
  data-section-id="{{ section.id }}"
>
  <div class="qsbw3__stage">
    <div class="qsbw3__grid" aria-hidden="true"></div>
    <div class="qsbw3__particles" aria-hidden="true"></div>

    <div class="qsbw3__media" aria-hidden="true">
      {% if is_slideshow %}
        <div class="qsbw3__slides" data-q-hero-slides>
          {% for block in section.blocks %}
            {% assign b = block.settings %}
            <div class="qsbw3__slide{% if forloop.first %} is-active{% endif %}" data-q-hero-slide {{ block.shopify_attributes }}>
              {% if b.media_type == 'video' and b.video_bg != blank %}
                {{ b.video_bg | video_tag:
                  image_size: '2048x',
                  autoplay: do_autoplay,
                  loop: M_loop,
                  muted: M_muted,
                  controls: M_controls,
                  playsinline: true
                }}
              {% elsif b.media_type == 'video_url' and b.video_url != blank %}
                <video class="qsbw3__video"
                  {% if do_autoplay %}autoplay{% endif %}
                  {% if M_loop %}loop{% endif %}
                  {% if M_muted %}muted{% endif %}
                  {% if M_controls %}controls{% endif %}
                  playsinline preload="metadata">
                  <source src="{{ b.video_url | escape }}" type="video/mp4">
                </video>
              {% elsif b.image_bg != blank %}
                {%- assign img = b.image_bg -%}
                <img
                  class="qsbw3__image"
                  srcset="
                    {{ img | image_url: width: 576 }} 576w,
                    {{ img | image_url: width: 960 }} 960w,
                    {{ img | image_url: width: 1440 }} 1440w,
                    {{ img | image_url: width: 1920 }} 1920w,
                    {{ img | image_url: width: 2560 }} 2560w
                  "
                  sizes="100vw"
                  src="{{ img | image_url: width: 1920 }}"
                  alt=""
                  loading="lazy"
                  decoding="async"
                >
              {% endif %}
              {% if b.overlay_enable %}
                <div class="qsbw3__overlay"
                  style="--qsbw3-grad-a: {{ b.grad_color_1 | default: '#121212' }};
                         --qsbw3-grad-b: {{ b.grad_color_2 | default: '#1f1f1f' }};
                         --qsbw3-overlay: {{ b.overlay_opacity | default: 45 | divided_by: 100.0 }};">
                </div>
              {% endif %}
            </div>
          {% endfor %}
        </div>
      {% else %}
        {% if s.bg_type == 'video' and s.video_bg != blank %}
          {{ s.video_bg | video_tag:
            image_size: '2048x',
            autoplay: do_autoplay,
            loop: M_loop,
            muted: M_muted,
            controls: M_controls,
            playsinline: true
          }}
        {% elsif s.bg_type == 'video_url' and s.video_url != blank %}
          <video class="qsbw3__video"
            {% if do_autoplay %}autoplay{% endif %}
            {% if M_loop %}loop{% endif %}
            {% if M_muted %}muted{% endif %}
            {% if M_controls %}controls{% endif %}
            playsinline preload="metadata">
            <source src="{{ s.video_url | escape }}" type="video/mp4">
          </video>
        {% elsif s.image_bg != blank %}
          {%- assign img = s.image_bg -%}
          <img
            class="qsbw3__image"
            srcset="
              {{ img | image_url: width: 576 }} 576w,
              {{ img | image_url: width: 960 }} 960w,
              {{ img | image_url: width: 1440 }} 1440w,
              {{ img | image_url: width: 1920 }} 1920w,
              {{ img | image_url: width: 2560 }} 2560w
            "
            sizes="100vw"
            src="{{ img | image_url: width: 1920 }}"
            alt=""
            loading="lazy"
            decoding="async"
          >
        {% endif %}
        {% if s.overlay_enable %}
          <div class="qsbw3__overlay"
            style="--qsbw3-grad-a: {{ s.grad_color_1 | default: '#0b0b12' }};
                   --qsbw3-grad-b: {{ s.grad_color_2 | default: '#111122' }};
                   --qsbw3-overlay: {{ s.overlay_opacity | default: 45 | divided_by: 100.0 }};">
          </div>
        {% endif %}
      {% endif %}
    </div>

    <div class="qsbw3__content-wrap">
      {% if is_slideshow %}
        <div class="qsbw3__content-carousel" data-q-hero-content aria-live="polite">
          {% for block in section.blocks %}
            {% assign b = block.settings %}
            <div class="qsbw3__content{% if forloop.first %} is-active{% endif %}" data-q-hero-content-slide {{ block.shopify_attributes }}>
              {% if b.kicker != blank %}
                <p class="qsbw3__kicker">{{ b.kicker }}</p>
              {% endif %}
              {% if b.heading != blank %}
                <h2 class="qsbw3__heading">{{ b.heading }}</h2>
              {% endif %}
              {% if b.subheading != blank %}
                <p class="qsbw3__subheading">{{ b.subheading }}</p>
              {% endif %}

              {% if b.cta_1_label != blank or b.cta_2_label != blank %}
                <div class="qsbw3__ctas" data-tab-group>
                  {% if b.cta_1_label != blank and b.cta_1_link != blank %}
                    <a
                      href="{{ b.cta_1_link }}"
                      class="q-btn {% if b.cta_1_style == 'solid' %}q-btn--solid{% elsif b.cta_1_style == 'outline' %}q-btn--outline{% elsif b.cta_1_style == 'ghost' %}q-btn--ghost{% endif %}"
                      data-variant="primary"
                    >
                      <span class="q-btn__label">{{ b.cta_1_label }}</span>
                    </a>
                  {% endif %}
                  {% if b.cta_2_label != blank and b.cta_2_link != blank %}
                    <a
                      href="{{ b.cta_2_link }}"
                      class="q-btn {% if b.cta_2_style == 'solid' %}q-btn--solid{% elsif b.cta_2_style == 'outline' %}q-btn--outline{% elsif b.cta_2_style == 'ghost' %}q-btn--ghost{% endif %}"
                      data-variant="secondary"
                    >
                      <span class="q-btn__label">{{ b.cta_2_label }}</span>
                    </a>
                  {% endif %}
                </div>
              {% endif %}
            </div>
          {% endfor %}
        </div>
      {% else %}
        <div class="qsbw3__content">
          {% if s.kicker != blank %}
            <p class="qsbw3__kicker">{{ s.kicker }}</p>
          {% endif %}
          {% if s.heading != blank %}
            <h2 class="qsbw3__heading">{{ s.heading }}</h2>
          {% endif %}
          {% if s.subheading != blank %}
            <p class="qsbw3__subheading">{{ s.subheading }}</p>
          {% endif %}

          {% if s.cta_1_label != blank or s.cta_2_label != blank %}
            <div class="qsbw3__ctas" data-tab-group>
              {% if s.cta_1_label != blank and s.cta_1_link != blank %}
                <a
                  href="{{ s.cta_1_link }}"
                  class="q-btn {% if s.cta_1_style == 'solid' %}q-btn--solid{% elsif s.cta_1_style == 'outline' %}q-btn--outline{% elsif s.cta_1_style == 'ghost' %}q-btn--ghost{% endif %}"
                  data-variant="primary"
                >
                  <span class="q-btn__label">{{ s.cta_1_label }}</span>
                </a>
              {% endif %}
              {% if s.cta_2_label != blank and s.cta_2_link != blank %}
                <a
                  href="{{ s.cta_2_link }}"
                  class="q-btn {% if s.cta_2_style == 'solid' %}q-btn--solid{% elsif s.cta_2_style == 'outline' %}q-btn--outline{% elsif s.cta_2_style == 'ghost' %}q-btn--ghost{% endif %}"
                  data-variant="secondary"
                >
                  <span class="q-btn__label">{{ s.cta_2_label }}</span>
                </a>
              {% endif %}
            </div>
          {% endif %}
        </div>
      {% endif %}
    </div>
  </div>

  <style>
    .q-sub-banner-web3.qsbw3 {
      --qsbw3-h: calc(var(--q-min-h, 60vh) + 40px);
      --qsbw3-pad-x: clamp(16px, 4vw, 56px);
      --qsbw3-pad-y: clamp(24px, 6vh, 80px);
      --qsbw3-text: var(--q-color-hero-fg, var(--color-foreground, #fff));
      --qsbw3-shadow: 0 8px 28px rgba(0,0,0,.35);
      --qsbw3-grid: radial-gradient(transparent 1px, rgba(255,255,255,.05) 1px);
      --qsbw3-grid-size: 24px 24px;
      --qsbw3-overlay: 0.45;
      --qsbw3-grad-a: #0b0b12;
      --qsbw3-grad-b: #111122;

      /* JSON-driven button styling defaults/fallbacks */
      --q-btn-bg: {{ s.cta_btn_bg | default: settings.color_primary | default: '#2563eb' }};
      --q-btn-text: {{ s.cta_btn_text | default: settings.text_on_color | default: '#ffffff' }};
      --q-btn-border: {{ s.cta_btn_border | default: settings.color_primary | default: '#2563eb' }};
      --q-btn-radius: {{ s.cta_btn_radius | default: settings.button_radius | default: 12 }}px;
      --q-btn-hover-opacity: {{ s.cta_btn_hover_opacity | default: settings.button_hover_opacity | default: 92 | divided_by: 100.0 }};

      position: relative;
      min-height: var(--qsbw3-h);
      display: grid;
      isolation: isolate;
      color: var(--qsbw3-text);
      background: var(--q-color-hero-bg, transparent);
    }
    .qsbw3.align-left  { text-align: left; }
    .qsbw3.align-center{ text-align: center; }
    .qsbw3.align-right { text-align: right; }

    .qsbw3__stage {
      position: relative;
      min-height: var(--qsbw3-h);
      overflow: clip;
      border-radius: var(--radius-2xl, 20px);
      margin-inline: var(--qsbw3-pad-x);
      margin-top: var(--q-spacing-4, 16px);
      margin-bottom: var(--q-spacing-6, 24px);
      box-shadow: var(--elev-6, var(--qsbw3-shadow));
      background: color-mix(in hsl, var(--q-color-surface, #0b0b12) 60%, transparent);
    }

    .qsbw3__grid {
      position: absolute; inset: 0;
      background-image:
        linear-gradient(180deg, rgba(255,255,255,.08) 0, transparent 50%),
        var(--qsbw3-grid);
      background-size: 100% 100%, var(--qsbw3-grid-size);
      opacity: .35;
      pointer-events: none;
      z-index: 1;
      mix-blend-mode: soft-light;
      filter: saturate(1.1);
    }

    .qsbw3__particles {
      position: absolute; inset: 0;
      background:
        radial-gradient(12px 12px at 10% 30%, rgba(0, 255, 255, .14), transparent 60%),
        radial-gradient(12px 12px at 80% 60%, rgba(255, 0, 200, .12), transparent 60%),
        radial-gradient(10px 10px at 50% 80%, rgba(0, 120, 255, .10), transparent 60%);
      animation: qsbw3-float 14s linear infinite alternate;
      pointer-events: none;
      z-index: 2;
      mix-blend-mode: screen;
    }
    @keyframes qsbw3-float {
      0% { transform: translate3d(0,0,0) scale(1); filter: blur(0px); }
      50%{ transform: translate3d(0,-6px,0) scale(1.02); filter: blur(.3px); }
      100%{ transform: translate3d(0,6px,0) scale(.98); filter: blur(.6px); }
    }

    .qsbw3__media { position: absolute; inset: 0; z-index: 0; pointer-events: none; overflow: hidden; border-radius: inherit; }
    .qsbw3__image, .qsbw3__video, .qsbw3__slides,
    .qsbw3__slide > .qsbw3__image, .qsbw3__slide > .qsbw3__video {
      position: absolute; inset: 0; width: 100%; height: 100%; object-fit: cover; display: block;
    }
    .qsbw3__slides { position: absolute; inset: 0; }
    .qsbw3__slide { position: absolute; inset: 0; opacity: 0; transition: opacity .6s ease; }
    .qsbw3__slide.is-active { opacity: 1; }

    .qsbw3__overlay {
      position: absolute; inset: 0;
      background:
        linear-gradient(180deg, color-mix(in oklab, var(--qsbw3-grad-a), transparent calc((1 - var(--qsbw3-overlay)) * 100%)), transparent),
        radial-gradient(120% 100% at 50% 0%, color-mix(in oklab, var(--qsbw3-grad-b), transparent calc((1 - var(--qsbw3-overlay)) * 100%)), transparent 70%);
      mix-blend-mode: overlay;
      z-index: 3;
      pointer-events: none;
    }

    .qsbw3__content-wrap {
      position: relative;
      z-index: 4;
      min-height: var(--qsbw3-h);
      display: grid;
      place-items: center;
      padding: var(--qsbw3-pad-y) var(--qsbw3-pad-x);
    }
    .qsbw3__content-carousel,
    .qsbw3__content { width: min(1100px, 92vw); margin-inline: auto; }
    .qsbw3__content-carousel [data-q-hero-content-slide] { display: none; }
    .qsbw3__content-carousel [data-q-hero-content-slide].is-active { display: block; animation: qsbw3-fade-in .5s ease both; }
    @keyframes qsbw3-fade-in { from { opacity: 0; transform: translateY(6px); } to { opacity: 1; transform: translateY(0); } }

    .qsbw3__kicker { font: var(--q-type-kicker, 600 clamp(.65rem, 1.4vw, .85rem) / 1.2 var(--font-body, ui-sans-serif)); letter-spacing: .08em; text-transform: uppercase; opacity: .9; margin: 0 0 .4rem 0; }
    .qsbw3__heading { font: var(--q-type-h2, 700 clamp(1.35rem, 3.4vw, 2rem) / 1.15 var(--font-heading, ui-sans-serif)); margin: 0 0 .6rem 0; text-wrap: balance; filter: drop-shadow(0 1px 0 rgba(0,0,0,.25)); }
    .qsbw3__subheading { font: var(--q-type-body-lg, 400 clamp(.9rem, 1.6vw, 1rem) / 1.6 var(--font-body, ui-sans-serif)); opacity: .92; margin: 0 0 1rem 0; text-wrap: pretty; }

    .qsbw3__ctas { display: inline-flex; gap: clamp(10px, 2vw, 16px); align-items: center; flex-wrap: wrap; }

    .q-sub-banner-web3 .q-btn{
      border-radius: var(--q-btn-radius);
      transition: transform .2s ease, box-shadow .2s ease, filter .2s ease, opacity .2s ease;
      position: relative;
      z-index: 5;
      outline-offset: 2px;
      box-shadow: 0 0 0 rgba(0,0,0,0);
      will-change: transform, filter;
    }
    .q-sub-banner-web3 .q-btn:not(.q-btn--outline):not(.q-btn--ghost),
    .q-sub-banner-web3 .q-btn.q-btn--solid{
      background: var(--q-btn-bg);
      color: var(--q-btn-text);
      border: var(--button_border_width, 1px) solid var(--q-btn-border);
    }
    .q-sub-banner-web3 .q-btn.q-btn--outline{
      background: transparent;
      color: var(--q-btn-border);
      border: var(--button_border_width, 1px) solid var(--q-btn-border);
    }
    .q-sub-banner-web3 .q-btn.q-btn--ghost{
      background: transparent;
      color: var(--q-btn-border);
      border: var(--button_border_width, 1px) solid transparent;
    }
    .q-sub-banner-web3 .q-btn:hover,
    .q-sub-banner-web3 .q-btn:focus-visible{
      transform: translateY(-1px);
      box-shadow: 0 8px 24px rgba(0,0,0,.25),
                  0 0 24px color-mix(in oklab, var(--q-btn-border), transparent 35%);
      filter: saturate(1.05);
      opacity: var(--q-btn-hover-opacity);
    }

    @media (prefers-color-scheme: light) {
      .q-sub-banner-web3.qsbw3 { --qsbw3-text: var(--q-color-hero-fg, #0b0b12); }
      .qsbw3__grid { opacity: .25; mix-blend-mode: multiply; }
      .qsbw3__overlay { mix-blend-mode: multiply; }
    }
    @media (prefers-reduced-motion: reduce) {
      .qsbw3__particles { animation: none; }
      .qsbw3__content-carousel [data-q-hero-content-slide].is-active { animation: none; }
    }
    @media (max-width: 720px) {
      .qsbw3.align-right .qsbw3__content, .qsbw3.align-left .qsbw3__content { text-align: center; }
    }

    /* Sub Banner: minimal look */
    .q-sub-banner-web3 .qsbw3__grid,
    .q-sub-banner-web3 .qsbw3__particles { display:none !important; } /* kill sparkles/grid */
    .q-sub-banner-web3 .qsbw3__overlay { mix-blend-mode: normal !important; } /* stop gray wash */
    .q-sub-banner-web3 .qsbw3__stage { box-shadow: none !important; border-radius: 12px !important; }

    /* Tighten buttons to look like proper CTAs */
    .q-sub-banner-web3 .q-btn { padding: .7rem 1rem !important; font-weight: 600 !important; }
    .q-sub-banner-web3 .q-btn.q-btn--outline { background: transparent !important; }

    /* Ensure text contrast over media without heavy haze */
    .q-sub-banner-web3 .qsbw3__content { text-shadow: 0 1px 0 rgba(0,0,0,.2); }
    .q-sub-banner-web3 .qsbw3__content {
    transform: translateY(0);
    transition: transform .4s ease, opacity .4s ease;
    }
    .q-sub-banner-web3:hover .qsbw3__content {
    transform: translateY(-2px);
    opacity: .98;
    }

  </style>

  {% if is_slideshow and s.slide_autoplay and respect_rm == false %}
    <script>
      (function(){
        const root = document.currentScript.closest('.q-sub-banner-web3');
        if(!root) return;
        const slides = root.querySelectorAll('[data-q-hero-slide]');
        const contents = root.querySelectorAll('[data-q-hero-content-slide]');
        if(slides.length <= 1) return;

        let idx = 0;
        const dur = {{ s.slide_interval | default: 5 | times: 1000 }};
        function setActive(i){
          slides.forEach((el, n) => el.classList.toggle('is-active', n === i));
          contents.forEach((el, n) => el.classList.toggle('is-active', n === i));
        }
        setActive(0);
        setInterval(() => { idx = (idx + 1) % slides.length; setActive(idx); }, dur);
      })();
    </script>
  {% endif %}
</section>

{% schema %}
{
  "name": "Sub Page Banner (Web3)",
  "tag": "section",
  "class": "q-sub-banner-web3",
  "settings": [
    { "type": "select", "id": "mode", "label": "Content mode", "default": "static", "options": [
      { "value": "static", "label": "Static (single background)" },
      { "value": "slideshow", "label": "Slideshow (up to 8 slides)" }
    ]},
    { "type": "text", "id": "aria_label", "label": "Accessible label", "default": "Sub page banner" },
    { "type": "select", "id": "align_h", "label": "Content alignment", "default": "center", "options": [
      { "value": "left", "label": "Left" },
      { "value": "center", "label": "Center" },
      { "value": "right", "label": "Right" }
    ]},

    { "type": "header", "content": "Static background" },
    { "type": "select", "id": "bg_type", "label": "Background type", "default": "image", "options": [
      { "value": "image", "label": "Image" },
      { "value": "video", "label": "Shopify-hosted video" },
      { "value": "video_url", "label": "External video URL (MP4)" }
    ]},
    { "type": "image_picker", "id": "image_bg", "label": "Background image" },
    { "type": "video", "id": "video_bg", "label": "Background video (Shopify file)" },
    { "type": "url", "id": "video_url", "label": "Background video URL (MP4)" },

    { "type": "checkbox", "id": "overlay_enable", "label": "Enable gradient overlay", "default": true },
    { "type": "color", "id": "grad_color_1", "label": "Gradient color A", "default": "#0b0b12" },
    { "type": "color", "id": "grad_color_2", "label": "Gradient color B", "default": "#111122" },
    { "type": "range", "id": "overlay_opacity", "label": "Overlay intensity", "default": 45, "min": 0, "max": 95, "step": 5 },

    { "type": "header", "content": "Static content" },
    { "type": "text", "id": "kicker", "label": "Kicker (small label)", "default": "Kicker" },
    { "type": "text", "id": "heading", "label": "Heading", "default": "Sub page banner — Web3" },
    { "type": "textarea", "id": "subheading", "label": "Subheading", "default": "A lighter, shorter Web3-styled banner for sub pages." },

    { "type": "text", "id": "cta_1_label", "label": "Primary CTA label" },
    { "type": "url", "id": "cta_1_link", "label": "Primary CTA link" },
    { "type": "select", "id": "cta_1_style", "label": "Primary CTA style", "default": "solid", "options": [
      { "value": "solid", "label": "Solid" },
      { "value": "outline", "label": "Outline" },
      { "value": "ghost", "label": "Ghost" }
    ]},

    { "type": "text", "id": "cta_2_label", "label": "Secondary CTA label" },
    { "type": "url", "id": "cta_2_link", "label": "Secondary CTA link" },
    { "type": "select", "id": "cta_2_style", "label": "Secondary CTA style", "default": "outline", "options": [
      { "value": "solid", "label": "Solid" },
      { "value": "outline", "label": "Outline" },
      { "value": "ghost", "label": "Ghost" }
    ]},

    { "type": "header", "content": "Button Styling" },
    { "type": "color", "id": "cta_btn_bg", "label": "Button background color", "default": "#2563eb" },
    { "type": "color", "id": "cta_btn_text", "label": "Button text color", "default": "#ffffff" },
    { "type": "color", "id": "cta_btn_border", "label": "Button border color", "default": "#2563eb" },
    { "type": "range", "id": "cta_btn_radius", "label": "Button radius (px)", "min": 0, "max": 32, "step": 1, "default": 12 },
    { "type": "range", "id": "cta_btn_hover_opacity", "label": "Hover opacity (×100)", "min": 60, "max": 100, "step": 1, "default": 92 },

    { "type": "header", "content": "Slideshow options" },
    { "type": "checkbox", "id": "slide_autoplay", "label": "Autoplay slides (disabled if 'Reduce motion' is on)", "default": true },
    { "type": "range", "id": "slide_interval", "label": "Autoplay interval (seconds)", "default": 5, "min": 3, "max": 12, "step": 1 }
  ],
  "blocks": [
    {
      "type": "slide",
      "name": "Slide",
      "limit": 8,
